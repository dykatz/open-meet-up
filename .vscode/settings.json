{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "npm.autoDetect": "off",
  "typescript.tsc.autoDetect": "off",
  "cSpell.words": [
    "daisyui",
    "nextauth",
    "Parens",
    "superjson",
    "tailwindcss",
    "trpc"
  ],
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  }
}
